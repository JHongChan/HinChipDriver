/dts-v1/;
/plugin/;

/ {
	fragment@0 {
		target-path = "/io-bus";
		__overlay__ {
			axi-usb2-device@60090000 {
				compatible = "xlnx,usb2-device-4.00.a";
				interrupt-parent = <&{/soc/interrupt-controller@60040000}>;
				interrupts = <0x0 0x1>;
				reg = <0x60090000 0x10000>;
				xlnx,has-builtin-dma;
			};
		};	
	};
};
